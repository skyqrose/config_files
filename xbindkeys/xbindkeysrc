# Increase volume
"/data/config_files/xbindkeys/sound.sh up"
    XF86AudioRaiseVolume

# Decrease volume
"/data/config_files/xbindkeys/sound.sh down"
    XF86AudioLowerVolume

# Toggle mute
"/data/config_files/xbindkeys/sound.sh mute"
    XF86AudioMute

"/data/config_files/xbindkeys/brightness.sh inc"
    XF86MonBrightnessUp

"/data/config_files/xbindkeys/brightness.sh dec"
    XF86MonBrightnessDown

"/data/config_files/scripts/projector reset"
    XF86Display

"/data/config_files/scripts/projector hdmi extend"
    XF86LaunchA

"/data/config_files/scripts/projector hdmi only"
    XF86Explorer
